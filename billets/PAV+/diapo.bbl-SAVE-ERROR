      \strng{namehash}{a2c243bfc87c7c76f2871cc7cccd56ac}
      \strng{fullhash}{f3b1780b04e699ed2be39f16e986a691}
      \strng{fullhashraw}{f3b1780b04e699ed2be39f16e986a691}
      \strng{bibnamehash}{a2c243bfc87c7c76f2871cc7cccd56ac}
      \strng{authorbibnamehash}{a2c243bfc87c7c76f2871cc7cccd56ac}
      \strng{authornamehash}{a2c243bfc87c7c76f2871cc7cccd56ac}
      \strng{authorfullhash}{f3b1780b04e699ed2be39f16e986a691}
