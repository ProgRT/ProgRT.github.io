  author = {Laghi, Franco and Cattapan, Steven E. and Jubran, Amal and Parthasarathy, Sairam and Warshawsky, Paul and Choi, Yoon-Sub A. and Tobin, Martin J.},
  year = {2003},
  month = jan,
  journal = {American Journal of Respiratory and Critical Care Medicine},
  volume = {167},
