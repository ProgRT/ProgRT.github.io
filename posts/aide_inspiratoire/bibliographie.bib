@article{shanely2002,
  title = {Mechanical {{Ventilation}}--Induced {{Diaphragmatic Atrophy Is Associated}} with {{Oxidative Injury}} and {{Increased Proteolytic Activity}}},
  author = {Shanely, R. Andrew and Zergeroglu, Murat A. and Lennon, Shannon L. and Sugiura, Takao and Yimlamai, Tossaporn and Enns, Debbie and Belcastro, Angelo and Powers, Scott K.},
  year = {2002},
