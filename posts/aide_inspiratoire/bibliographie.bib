@article{al-husinat2024,
  title = {The {{Role}} of {{Ultrasonography}} in the {{Process}} of {{Weaning}} from {{Mechanical Ventilation}} in {{Critically Ill Patients}}},
  author = {{Al-Husinat}, Lou'i and Jouryyeh, Basil and Rawashdeh, Ahlam and Robba, Chiara and Silva, Pedro Leme and Rocco, Patricia Rieken Macedo and Battaglini, Denise},
  year = {2024},
  month = feb,
  journal = {Diagnostics},
  volume = {14},
  number = {4},
  pages = {398},
  issn = {2075-4418},
  doi = {10.3390/diagnostics14040398},
  urldate = {2024-08-30},
  abstract = {Weaning patients from mechanical ventilation (MV) is a complex process that may result in either success or failure. The use of ultrasound at the bedside to assess organs may help to identify the underlying mechanisms that could lead to weaning failure and enable proactive measures to minimize extubation failure. Moreover, ultrasound could be used to accurately identify pulmonary diseases, which may be responsive to respiratory physiotherapy, as well as monitor the effectiveness of physiotherapists' interventions. This article provides a comprehensive review of the role of ultrasonography during the weaning process in critically ill patients.},
  pmcid = {PMC10888003},
